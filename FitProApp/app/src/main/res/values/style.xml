<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Tu estilo personalizado -->
    <style name="CustomBottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <!-- Ajusta el padding interno -->
        <item name="itemPaddingTop">0dp</item>
        <item name="itemPaddingBottom">0dp</item>

        <!-- Ajusta el tamaño de los íconos -->
        <item name="itemIconSize">30dp</item>

        <!-- Opcional: ajusta los colores -->
        <item name="itemTextColor">@color/selector_nav_colors</item>
        <item name="itemIconTint">@color/selector_nav_colors</item>

    </style>
</resources>
